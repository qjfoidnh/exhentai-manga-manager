{
  "name": "exhentai-manga-manager",
  "private": true,
  "version": "1.3.7",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "chcp 65001 && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "electron.e-hentai.tagger",
    "files": [
      "dist/index.html",
      "dist/assets/*",
      "fileLoader/*",
      "index.js",
      "preload.js"
    ],
    "extraResources": [
      {
        "from": "resources/extraResources",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "glob": "^7.2.0",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.2",
    "sharp": "^0.30.3",
    "superagent": "^7.1.2",
    "superagent-proxy": "^3.0.0"
  },
  "devDependencies": {
    "@vicons/ionicons4": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "axios": "^0.26.1",
    "electron": "^18.0.3",
    "electron-builder": "^22.5.1",
    "element-plus": "^2.1.8",
    "he": "^1.2.0",
    "stylus": "^0.57.0",
    "vite": "^2.9.0",
    "vue": "^3.2.25",
    "vuedraggable": "^4.1.0"
  }
}
